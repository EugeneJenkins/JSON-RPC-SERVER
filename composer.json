{
    "name": "eugene/json-rpc-server",
    "description": "JSON-RPC server",
    "type": "library",
    "autoload": {
        "psr-4": {
            "EugeneJenkins\\JsonRpcServer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Eugene Jenkins",
            "email": "evgheni.chiurcciu@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.1."
    },
    "require-dev": {
        "phpstan/phpstan": "^1.12",
        "phpunit/phpunit": "^11.3"
    },
    "scripts": {
        "code-lint": "vendor/bin/phpstan analyse -c phpstan.neon",
        "test": "./vendor/bin/phpunit tests"
    }
}
