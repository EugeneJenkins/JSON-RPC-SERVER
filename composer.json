{
    "name": "eugenejenkins/json-rpc-server",
    "description": "A lightweight JSON-RPC server implementation in PHP.",
    "type": "library",
    "keywords": ["json-rpc", "php", "server", "rpc"],
    "license": "MIT",
    "homepage": "https://github.com/EugeneJenkins/json-rpc-server",
    "authors": [
        {
            "name": "Eugene Jenkins",
            "email": "evgheni.chiurcciu@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "EugeneJenkins\\JsonRpcServer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EugeneJenkins\\JsonRpcServer\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=8.1."
    },
    "require-dev": {
        "phpstan/phpstan": "^1.12",
        "phpunit/phpunit": "^11.3"
    },
    "scripts": {
        "code-lint": "vendor/bin/phpstan analyse -c phpstan.neon",
        "test": "./vendor/bin/phpunit tests"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
